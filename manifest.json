{
	"name": "FlyingVessel",
	"version": "0.2.1",
	"description": "Makes Floatplane a better place.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"http://*.floatplane.com/*",
				"https://*.floatplane.com/*",
				"http://*.floatplane.club/*",
				"https://*.floatplane.club/*"
			],
			"run_at": "document_idle",
			"js": ["script.js"]
		}
	],
	"permissions": [
        "activeTab",
		"storage"
    ]
}